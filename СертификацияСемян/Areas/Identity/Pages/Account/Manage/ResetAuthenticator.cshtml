@page
@model ResetAuthenticatorModel
@{
    ViewData["Title"] = Manage.ОбнулитьКлюч;
    ViewData["ActivePage"] = ManageNavPages.TwoFactorAuthentication;
}

<partial name="_StatusMessage" for="StatusMessage" />
<h3>@ViewData["Title"]</h3>
<div class="alert alert-warning" role="alert">
    <p>
        <span class="glyphicon glyphicon-warning-sign"></span>
        <strong>@Manage.ЕслиОбнулитеКлюч</strong>
    </p>
    <p>
        @Manage.ПроцессОтключает2ФА
    </p>
</div>
<div>
    <form id="reset-authenticator-form" method="post">
        <button id="reset-authenticator-button" class="btn btn-danger" type="submit">@Manage.ОбнулитьКлюч</button>
    </form>
</div>
